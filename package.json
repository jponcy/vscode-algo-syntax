{
    "name": "algo-tact",
    "publisher": "jponcy",
    "displayName": "algo",
    "description": "Algorithm syntax",
    "version": "0.2.4",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jponcy/vscode-algo-syntax.git"
    },
    "contributes": {
        "languages": [
            {
                "id": "algo",
                "aliases": [
                    "Algorithm",
                    "algo"
                ],
                "extensions": [
                    "algo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "algo",
                "scopeName": "source.algo",
                "path": "./syntaxes/algo.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "algo",
                "path": "./snippets.json"
            }
        ]
    }
}
